[user]
    name = Zakhar Pilipchuk
    email = zahaaar@yandex-team.ru
[credential]
    helper = cache --timeout=3600
[core]
    editor = vim
[alias]
    tree = log --graph --abbrev-commit --decorate --date=relative --format=format:\
\"%C(bold blue)%h%C(reset) %C(bold blue)(%ar)%C(reset)\"\
\" %C(white)%s%C(reset) %C(dim white)- %an%C(reset)\"\
\" %C(bold yellow)%d%C(reset)\" --all
    co = commit --verbose
    st = status
